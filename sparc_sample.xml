<?xml version="1.0"?>
<oES>
	<system>
		<name> Online SPARC </name>
	</system>
	<config>  
		<option name="underlying solver"> 
			<commandSwitch> -solver </commandSwitch> 
			<optionvalues>
				<li>Clingon</li>
				<li>DLV</li>
			</optionvalues>
		</option>
		<option name="Number of Answer Sets"> 
			<commandSwitch> -numAnswerSets </commandSwitch>
			<optionInput>10</optionInput>
		</option> 
	</config>
	 <navbar>
		<right>
            <button>
                <inputField>enter query</inputField>
                <inputField>enter tomatoes</inputField>
                <text>Submit</text>
                <id>submitBut</id>
                <behavior>          
                    <program> sparc </program>
                    <editor></editor>
                    <inputFieldId>query</inputFieldId>
                    <inputFieldId>tomatoes</inputFieldId>
                </behavior>
            </button>
			<button>
				<text>Draw</text>
				<id>drawBut</id>
				<behavior>
					<program> sparc </program> 
					<editor></editor> 
				</behavior>	
			</button>
            <button>
                <text>Animate</text>
                <id>animateBut</id>
                <behavior>
                    <program> animate </program> 
                    <editor></editor> 
                </behavior>
            </button>

		</right>
	</navbar>
</oES>
